  <appSettings>
    
    <add key="DefaultWatchedFolder" value="D:\epam_temp\"/>    
    
    <add key="CheckProducts" value="false"/>
    <add key="CheckManagers" value="false"/>
    
    <add key="ManagerSing" value="Manager"/>
    <add key="ProductSing" value="Product"/>
    <add key="ClientSing" value="Client"/>
